  "scripts": {
    "server:build": "node_modules/.bin/rimraf dist && nest build",
    "server:dev": "set NODE_ENV=development&& nest start --watch",
    "server:debug": "set NODE_ENV=development&& nest start --debug --watch",
    "server:prod": "node dist/main",
    "client:dev": "parcel watch \"client/manifest.html\" --out-dir \"public/build\" --public-url \"/build/\"",
    "client:build": "node_modules/.bin/rimraf public/build && parcel build \"client/manifest.html\" --out-dir \"public/build\" --public-url \"/build/\"",
    "format": "prettier --write \"{server,client}/**/*.{ts,js}\"",
    "lint:client": "eslint \"client/**/*.js\" --fix",
    "lint:server": "eslint \"server/**/*.ts\" --fix",
    "start": "NODE_ENV=production npm run server:prod",
    "build": "npm run server:build && npm run client:build",
    "test": "jest"
  },
